{
  "name": "firebase-tutorial-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.default next dev",
    "build": "env-cmd -f .env.default next build",
    "start": "env-cmd -f .env.default next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "deploy": "rm -rf .firebase && yarn build && next-csr-rewrites-generate --format firebase && yarn format && firebase deploy --project default"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@sonicgarden/next-csr-rewrites-generator": "^1.2.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
